      <script>
          function getOfferIcon(category){
              switch(category){
                  case 'Electronics':
                      return 'icon_electronicsBlue';
                  case 'Gold':
                      return 'icon_goldBlue';
                  case 'Travel':
                      return 'icon_travel';
                  //Use  case 'Furnishing/ Furniture' for sanity
                  case 'Furnishing_Furniture':
                      return 'icon_furniture';
                  case 'Automobiles':
                      return 'icon_car';
                  default:
                      return 'icon_othersBlue';
              }
          }
          function setIcon(){
              //debugger;
              let offers = document.querySelectorAll('.offerIcon');
              offers.forEach(offer =>{
                  offer.classList.add(getOfferIcon(offer.dataset?offer.dataset['category']:''));
              })
          }
      </script>
