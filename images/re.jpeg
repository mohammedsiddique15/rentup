    <script>
    
        function moveNLPDCard(direction) {
            let obj = {
                parentId: 'nlpsrpDeskId',
                id: 'carouselCard_nlpsrp',
                carouselClass: 'xidRecommendation__xidRecommCard',
                rightBtnCls: 'rightBtn',
                leftBtnCls: 'leftBtn',
                moveTupple: 2,
                direction
            },
                attributes = {};
    
            !attributes.parentDom && (attributes = getAttributes(obj)());
            let { domElement, rightBtn, leftBtn, singleElement, moveTupple, margin, cardsWidth } = attributes;
    
            (domElement.clientWidth > (cardsWidth - (direction == 'right' ? 0 : domElement.scrollLeft + moveTupple * singleElement.clientWidth + moveTupple * margin))) ? (rightBtn[0].style.display = 'none') : rightBtn[0].style.display = '';
    
    
            if (direction == 'left') {
                domElement.scrollTo({
                    left: domElement.scrollLeft + moveTupple * singleElement.clientWidth + (moveTupple) * margin,
                    behavior: 'smooth',
                })
                leftBtn[0].style.display = 'inline';
            } else {
                let flag = domElement.scrollLeft < singleElement.clientWidth * 2;
                (domElement.scrollLeft - moveTupple * singleElement.clientWidth) < (singleElement.clientWidth - moveTupple * margin) && (leftBtn[0].style.display = 'none');
                domElement.scrollTo({
                    left: flag ? 0 : domElement.scrollLeft - moveTupple * singleElement.clientWidth - (moveTupple) * margin,
                    behavior: 'smooth',
                })
            }
        };
    
    
        function getAttributes(obj) {
            let { parentId = '', id = '', carouselClass = '', rightBtnCls = '', leftBtnCls = '', moveTupple = 1 } = obj;
    
            let parentDom = document.getElementById(parentId),
                domElement = document.getElementById(id),
                elements = domElement.getElementsByClassName(carouselClass),
                rightBtn = parentDom.getElementsByClassName(rightBtnCls),
                leftBtn = parentDom.getElementsByClassName(leftBtnCls),
                singleElement = elements[0],
                margin = parseInt(getComputedStyle(singleElement).marginLeft) + parseInt(getComputedStyle(singleElement).marginRight) + 5,
                cardsWidth = (singleElement.clientWidth) * elements.length + elements.length * margin;
    
            function attributeObj() {
                return {
                    parentDom, domElement, elements, rightBtn, leftBtn, singleElement, moveTupple, margin, cardsWidth
                }
            }
    
            return attributeObj;
        };
    </script>
    
