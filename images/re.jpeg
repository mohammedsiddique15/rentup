<script>
    var curr = 0

    function carouselSlideInlineBanner(type) {
        let rootElement = document.querySelector('#INLINE_BANNER_comp_wrapper_client .SingleSlideSwipe__SingleSlideStrem_Inline_Banner');
        let subElements = document.querySelectorAll('#INLINE_BANNER_comp_wrapper_client .featuredProjectsCard__featuredProjectCard_Inline_Banner');
        if (rootElement && subElements && subElements.length > 0) {
            let subElementWidth = subElements[0].offsetWidth;
            let rootWidth = rootElement.offsetWidth;
            console.log(rootWidth);

            let subElemRightMargin = 0;
            let subElemStyle = subElements[0].currentStyle || window.getComputedStyle(subElements[0]);
            if(subElemStyle && subElemStyle.marginRight){
                subElemRightMargin = parseFloat(subElemStyle.marginRight);
            }

            let prev = document.querySelector('.featuredProjectsCard__SliderPrev_Inline_Banner');
            let next = document.querySelector('.featuredProjectsCard__SliderNxt_Inline_Banner');

            let subElementsTotalWidth = (subElementWidth+subElemRightMargin) * subElements.length;
            console.log("Sub elements total width ", subElementsTotalWidth);
            console.log("Root element width ", rootWidth);
            if (type == "next") {
                curr -= (subElementWidth+subElemRightMargin);
                console.log(curr)
                rootElement.style.transform = 'translate(' + (curr) + 'px,0)'
            }
            else if (type == "prev") {
                curr += (subElementWidth+subElemRightMargin);
                if (curr > 0) {
                    curr = 0;
                }
                console.log(curr)
                rootElement.style.transform = 'translate(' + (curr) + 'px,0)'
            } else {
                return;
            }

            if (curr < 0) {
                prev.style.display = "block";
            } else {
                prev.style.display = "none";
            }

            if ((rootWidth-curr) < subElementsTotalWidth) {
                next.style.display = "block";
            }
            else {
                next.style.display = "none";
            }
        }
    }
</script>

