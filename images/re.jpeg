<script type="text/javascript">
    function calculateFilmWidth() {
        var cardElement = document.getElementsByClassName('projectInFocusNew__filmOfSlides')[0];
        var sliderElement = document.getElementsByClassName('projectInFocusNew__barsContainer')[0];
        var target = document.querySelectorAll('.projectInFocusNew__filmsheet')
        var elementWidth = (cardElement.offsetWidth) * target.length // add count variable here
        cardElement.style.transform = "translateX(0px)";
        window.elementWidth = elementWidth;
        window.cardElement = cardElement;
        window.target = target;
        cardElement.addEventListener("touchstart", handleStart, false);
        cardElement.addEventListener("touchend", handleEnd, false);

        for (var i = 0; i < target.length; i++) {
            target[i].style.width = elementWidth / target.length + 'px';

        }
        window.decrement = 0;
        window.index = 0;
        var slider = document.getElementsByClassName('projectInFocusNew__slide')
        slider[0].style.width = "100%"
        slider[0].style.opacity = "1"
        window.slider = slider;
        window.decrement -= elementWidth / target.length;
        window.myTimer = setInterval(intervalFunc, 5000);

    }
</script>
<script type="text/javascript">
    function moveCardNew(position) {
        var slider = document.getElementsByClassName('projectInFocusNew__slide')


        if (Math.abs(window.decrement) >= window.elementWidth && position == 'right') {
            window.decrement = 0;
            window.index = 0
            for (var i = 0; i < slider.length; i++) {
                window.slider[i].style.transition = 'none';
                slider[i].style.width = '0';
                slider[i].style.opacity = "0";

            }
        }
        // if ((window.decrement) >= 0) {
        //     window.decrement = -window.elementWidth;
        //     window.index = window.target.length
        //     for (var i = 0; i < slider.length; i++) {
        //         slider[i].style.width = '100%';
        //         slider[i].style.opacity = "1";
        //     }
        // }
        var oldDec = 0

        if (position == 'left') {


            if (window.index > 0) {
                oldDec = window.decrement + window.elementWidth / window.target.length;
                window.decrement += window.elementWidth / window.target.length;
                window.decrement -= window.decrement / window.index;
                window.index == (window.target.length - 1) && (window.flag = true)
                for (i = window.index; i >= window.index - 1; i--) {
                    slider[i].style.transition = 'none';
                    slider[i].style.width = "0"
                    slider[i].style.opacity = "0"

                }
                window.index -= 1;
                // window.index = Math.floor((Math.abs(oldDec) / window.elementWidth) * target.length);
                if (slider[window.index]) {
                    setTimeout(() => {
                        slider[window.index].style.transition = 'width 4.5s ease-in-out';
                        slider[window.index].style.width = "100%"
                        slider[window.index].style.opacity = "1"

                    }, 100)


                    cardElement.style.transform = 'translateX(' + (window.decrement) + 'px)';
                    window.decrement -= window.elementWidth / window.target.length;

                }
            } else {
                window.decrement = 0
                window.index = 0
                if (slider[0]) {
                    slider[0].style.transition = 'none';
                    slider[0].style.width = "0"
                    slider[0].style.opacity = "0"
                    slider[0].style.transition = 'width 4.5s ease-in-out';
                    setTimeout(() => {
                        slider[0].style.width = "100%"
                        slider[0].style.opacity = "1"

                    }, 3500);


                }
            }

        }
        if (position == 'right') {
            oldDec = window.decrement
            // window.decrement -= target.length;
            if (window.index < window.target.length - 1) {
                window.slider[window.index].style.transition = 'none';
                window.index += 1;

            }
            cardElement.style.transform = 'translateX(' + (window.decrement) + 'px)';
            window.decrement -= window.elementWidth / window.target.length;
            window.index = Math.floor((Math.abs(oldDec) / window.elementWidth) * target.length);
            if (slider[window.index]) {
                setTimeout(() => {

                    window.slider[window.index].style.transition = 'width 4.5s ease-in-out';
                    slider[window.index].style.width = "100%"
                    slider[window.index].style.opacity = "1"

                }, 10)

            }
        }

        clearInterval(window.myTimer);
        window.myTimer = setInterval(intervalFunc, 5000);

        // if (window.decrement < 0) {
        //     window.decrement = window.decrement - target.length;
        //     oldDec = window.decrement + target.length;
        // }




    }
</script>
