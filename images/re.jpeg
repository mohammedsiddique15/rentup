<script type="text/javascript">
    function intervalFunc() {
        var itemsLength = window.target.length;
        if (Math.abs(window.decrement) >= window.elementWidth) {
            window.decrement = 0;
            window.index = 0
            var slider = document.getElementsByClassName('projectInFocusNew__slide');
            setTimeout(() => {
                window.slider[window.index].style.transition = 'width 4.5s ease-in-out';
                slider[0].style.width = "100%";
                slider[0].style.opacity = "1";

            }, window.timeInterval || 800)
            window.timeInterval = undefined;
            for (var i = 0; i < window.slider.length; i++) {
                window.slider[i].style.transition = 'none';
                window.slider[i].style.width = '0';
                window.slider[i].style.opacity = "0";
            }
        }
        var oldDec = 0;

        if (window.decrement <= 0) {
            window.decrement = window.decrement - itemsLength;
            oldDec = window.decrement + itemsLength;
        }
        window.cardElement.style.transform = 'translateX(' + (window.decrement + itemsLength) + 'px)';
        window.decrement -= (window.elementWidth / itemsLength) - itemsLength;
        window.index = Math.floor((Math.abs(oldDec) / window.elementWidth) * itemsLength);
        if (window.slider[window.index]) {
            window.slider[window.index].style.transition = 'width 4.5s ease-in-out';
            if (window.index == (window.target && (itemsLength - 1) || 2)) {
                setTimeout(() => {
                    // var slider = document.getElementsByClassName('projectInFocusNew__slide')
                    if (!window.flag) {
                        window.slider[0].style.width = "0"
                        window.slider[0].style.opacity = "0"
                        window.slider[0].style.transition = 'none';

                    }
                    window.flag = false;
                }, 4900)
            }
            if (window.index !== 0) {

                window.slider[window.index].style.width = "100%"
                window.slider[window.index].style.opacity = "1"

            }
        }
    }
</script>
