<script>
    function commonLazyLoad(cls, className) {
        try {
            var lazyImages = [].slice.call(cls[0].querySelectorAll(["img", className].join(".")));
            if ("IntersectionObserver" in window) {
                let lazyImageObserver = new IntersectionObserver(function (entries, observer) {
                    entries.forEach(function (entry) {
                        if (entry.isIntersecting) {
                            let lazyImage = entry.target;
                            lazyImage.src = lazyImage.dataset.src;
                            lazyImage.classList.remove(className);
                            lazyImageObserver.unobserve(lazyImage);
                        }
                    });
                });

                lazyImages.forEach(function (lazyImage) {
                    lazyImageObserver.observe(lazyImage);
                });
            } else {
                let active_MNLP = false;

                const lazyLoad = function () {
                    if (active_MNLP === false) {
                        active_MNLP = true;

                        setTimeout(function () {
                            lazyImages.forEach(function (lazyImage) {
                                if ((lazyImage.getBoundingClientRect().top <= window.innerHeight && lazyImage.getBoundingClientRect().bottom >= 0) && getComputedStyle(lazyImage).display !== "none") {
                                    lazyImage.src = lazyImage.dataset.src;
                                    lazyImage.classList.remove(className);

                                    lazyImages = lazyImages.filter(function (image) {
                                        return image !== lazyImage;
                                    });

                                    if (lazyImages.length === 0) {
                                        document.removeEventListener("scroll", lazyLoad);
                                    }
                                }
                            });

                            active_MNLP = false;
                        }, 200);
                    }
                };

                document.addEventListener("scroll", lazyLoad);
            }
        }
        catch (err) {
            console.log("error in FP_SRP velocity templte", err);
        }

    }
</script>
<script type="text/javascript">
    function lazyload_DFP() {
        
        let cls = document.getElementsByClassName('featuredCardContainer__featuredCardContainerCompo');
        commonLazyLoad(cls, "lazyLoadImg");

    }
    if (document.readyState === 'loading') {
        

        // when DOMContentLoaded still Loading
        document.addEventListener('DOMContentLoaded', lazyload_DFP);
    } else {
        
        //when  `DOMContentLoaded` has already loaded
        // setTimeout(lazyload_DFP(), 4000);
        
        setTimeout(function () {
            lazyload_DFP();
        }, 2000);

        

    }

</script>




<script type="text/javascript">
    function doTracking(projectId, resCom, addRem) {
        var data = {
            "trackingData": {
                "action": {
                    "page": "HP", "stage": "ADD", "event": "SHORTLIST"
                },
                "payload": {
                    "project": {
                        "xid": "", "res_com": ""
                    }
                }
            }
        };
        data.trackingData.action.stage = addRem ? 'ADD' : 'REMOVE';
        data.trackingData.payload.project.res_com = resCom;
        data.trackingData.payload.project.xid = projectId;
        fetch("/do/clickStreamTracking/ClickStream/trackData", {
            method: 'POST',
            credentials: 'same-origin',
            headers: {
                'Content-Type': 'application/x-www-form-urlencoded',
            },
            body: 'trackingData[]=' + encodeURIComponent(JSON.stringify(data.trackingData)),
        })
    }
</script>
<script>
    function doShortlistFp(e, projectId, resCom) {

        e.stopPropagation();
        e.preventDefault();
        doTracking(projectId, resCom, e.target.classList.contains('icon_whiteShortlist'));
        var target = e.target;
        var status = e.target.classList.contains('icon_whiteShortlist');
        e.target.classList.add(status ? 'icon_whiteShortlisted' : 'icon_whiteShortlist');
        e.target.classList.remove(status ? 'icon_whiteShortlist' : 'icon_whiteShortlisted');
        if(window.desktopHpShortlistCount == null || window.desktopHpShortlistCount == 'undefined'){
            window.desktopHpShortlistCount = 0;
        }
        if(status){
            window.desktopHpShortlistCount = window.desktopHpShortlistCount? window.desktopHpShortlistCount + 1 : window.desktopHpShortlistCount;
        }else{
            window.desktopHpShortlistCount = window.desktopHpShortlistCount? window.desktopHpShortlistCount - 1 : window.desktopHpShortlistCount;
        }
        fetch("/do/shortlist/ShortlistProject/" + (status ? "ajaxShortlistInsert?" : "ajaxShortlistRemove?") + "id=" + projectId + "&res_com=" + resCom + "&noxid=Y&isAjax=true");


    }
